{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/how-to-turn-dev-to-blogs-into-viral-tweets",
      "dest": "/how-to-turn-dev-to-blogs-into-viral-tweets.html"
    },
    {
      "src": "/building-a-whatsapp-ai-health-and-fitness-agent-with-motia-and-openai",
      "dest": "/building-a-whatsapp-ai-health-and-fitness-agent-with-motia-and-openai.html"
    },
    {
      "src": "/building-a-lightning-fast-image-resizer-with-motia-and-aws-s3",
      "dest": "/building-a-lightning-fast-image-resizer-with-motia-and-aws-s3.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
